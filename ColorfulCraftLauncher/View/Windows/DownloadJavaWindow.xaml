<hc:Window x:Class="ColorfulCraftLauncher.View.Windows.DownloadJavaWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ColorfulCraftLauncher.View.Windows"
        mc:Ignorable="d"
        Title="DownloadJavaWindow" Height="250" Width="500">
    <Grid Margin="0,0,0,-5" Width="500">
        <ProgressBar x:Name="pb1" HorizontalAlignment="Center" Height="10" Margin="0,110,0,0" VerticalAlignment="Top" Width="400"/>
        <Button Content="安装Java" VerticalAlignment="Top" Height="50" Width="272" Command="{Binding DownloadJavaCommand}" Style="{Binding Mode=OneWay, Source={StaticResource ButtonSuccess}}" Margin="0,162,0,0" Click="Button_Click"/>

    </Grid>
</hc:Window>
